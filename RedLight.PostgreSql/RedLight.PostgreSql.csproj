<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<AssemblyName>RedLight.PostgreSql</AssemblyName>
		<TargetFrameworks>net8.0</TargetFrameworks>
		<DefineConstants></DefineConstants>

		<TieredPGO>true</TieredPGO>

		<PackageId>RedLight.PostgreSql</PackageId>
		<PackageTags>sql;sql-builder;orm;mini-orm;ado-net</PackageTags>
		<PackageProjectUrl>https://github.com/trs4/RedLight</PackageProjectUrl>
		<RepositoryUrl>https://github.com/trs4/RedLight</RepositoryUrl>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Npgsql" Version="9.0.3" />
		<PackageReference Include="System.Memory" Version="4.6.3" />
		<PackageReference Include="System.Threading.Tasks.Extensions" Version="4.6.3" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\RedLight\RedLight.csproj" />
	</ItemGroup>
</Project>
